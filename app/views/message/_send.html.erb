<table class="table">
  <thead>
    <tr>
      <th class="name">头像</th>
      <th>id</th>
      <th>性别</th>
      <th>状态</th>
      <th>最近登录</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% send_msgs.each do |c| %>
      <tr>  
        <td class="pic"><%= image_tag(c.receiver.avatar)%></td>
        <td> <%= link_to  c.receiver.username, showuser_path(c.receiver.id) %></td>
        <td><%= c.receiver.gender %></td>
        <td><%= c.receiver.status %></td>
        <td><%= time_ago_in_words c.created_at %> ago</td>
      </tr>
      <tr>
        <td><%= c.message.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>   